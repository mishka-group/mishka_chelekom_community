  <.custom_command code="mix mishka.ui.add component_navbar_003" />
  <.community_navbar_003 id="community_navbar_003-1" brand="Mishka">
    <:item>
      <.link>Home</.link>
    </:item>
    <:item>
      <.link>Media</.link>
